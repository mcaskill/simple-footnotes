{
    "name": "mcaskill/wp-simple-footnotes",
    "description": "A simple way to add footnotes using WordPress shortcodes.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Chauncey McAskill",
            "email": "chauncey@mcaskill.ca",
            "homepage": "https://mcaskill.ca"
        },
        {
            "name": "Andrew Nacin",
            "homepage": "https://github.com/nacin"
        }
    ],
    "support": {
        "issues": "https://github.com/mcaskill/wp-simple-footnotes/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "automattic/phpcs-neutron-ruleset": "^3.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
